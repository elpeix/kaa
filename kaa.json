{
  "name": "Sample server",
  "version": "1.0.0",
  "server": "example.SampleServer",
  "host": "localhost",
  "port": 5111,
  "debug": true,
  "enableCors": false,

  "developmentPolling": {
    "enabled": true,
    "intervalSeconds": 1,
    "include": ["kaa", "example", ".", "*.py", "*.json"],
    "exclude": [
      "docs",
      "tests",
      "__pycache__",
      "*.pyc",
      "*.md",
      "*.log",
      "LICENSE"
    ]
  }
}
